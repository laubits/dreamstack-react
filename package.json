{
  "name": "telos-react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@theme-ui/match-media": "^0.3.1",
    "@types/react-router-dom": "^5.1.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "framer-motion": "^1.8.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "i18next": "^19.1.0",
    "lint-staged": "^10.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^1.2.5",
    "react-i18next": "^11.3.1",
    "react-icons": "^3.9.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "subscriptions-transport-ws": "^0.9.16",
    "theme-ui": "^0.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(js|ts|tsx)\"",
    "check-types": "tsc",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "check-format": "yarn prettier --list-different",
    "validate": "yarn check-types && yarn check-format && yarn lint && yarn build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@blockmatic/eosio-hooks": "^0.1.7",
    "@blockmatic/eslint-config": "^1.3.5",
    "@blockmatic/prettier-config": "^1.2.3",
    "@blockmatic/tsconfig": "^1.7.0",
    "@types/jest": "24.9.1",
    "@types/node": "13.5.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/theme-ui": "^0.2.7",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.1",
    "prettier": "^1.19.1",
    "typescript": "3.7.5"
  }
}
